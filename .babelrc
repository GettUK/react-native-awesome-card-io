{
  "presets": ["react-native"],
  "plugins": [
    "lodash",
    [
      "module-resolver",
      {
        "extensions": [".js", ".ios.js", ".android.js"],
        "alias": {
          "actions": "./src/actions",
          "api": "./src/api",
          "assets": "./src/assets",
          "components": "./src/components",
          "config": "./src/config",
          "containers": "./src/containers",
          "locales": "./src/locales",
          "navigators": "./src/navigators",
          "reducers": "./src/reducers",
          "store": "./src/store",
          "theme": "./src/theme",
          "utils": "./src/utils"
        }
      }
    ]
  ],
  "retainLines": true
}
